{
  "name": "mu-engine",
  "version": "0.11.1",
  "description": "mu-engine",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jjg1914/mu-engine.git"
  },
  "author": "John J. Glynn IV",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jjg1914/mu-engine/issues"
  },
  "homepage": "https://github.com/jjg1914/mu-engine#readme",
  "scripts": {
    "prepublish": "yarn run build",
    "build": "tsc",
    "test": "nyc mocha",
    "test-watch": "npm run test -- --watch --watch-extensions ts -G",
    "lint": "tslint --project .",
    "check": "npm run build && npm run test && npm run lint",
    "coverage": "coveralls < coverage/lcov.info",
    "clean": "rm -rf lib"
  },
  "engines": {
    "node": ">= 8.9.4",
    "npm": "^5.7.1",
    "yarn": "^1.5.1"
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary",
      "lcov"
    ],
    "sourceMap": true,
    "instrument": true
  },
  "devDependencies": {
    "@types/chai": "^4.0.5",
    "@types/mocha": "^2.2.44",
    "@types/sinon": "^4.0.0",
    "@types/sinon-chai": "^2.7.29",
    "chai": "^4.1.2",
    "coveralls": "^3.0.0",
    "jsdom": "^9.0",
    "mocha": "^5.0.1",
    "mocha-jsdom": "^1.1.0",
    "nyc": "^11.3.0",
    "sinon": "^4.1.2",
    "sinon-chai": "^2.14.0",
    "source-map-support": "^0.5.0",
    "ts-node": "^5.0.1",
    "tslint": "^5.8.0",
    "tslint-config-standard": "^7.0.0",
    "typescript": "^2.4.1"
  },
  "dependencies": {
    "json": "^9.0.6",
    "npm": "^5.7.1"
  }
}
